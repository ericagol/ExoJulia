    "function rv_loglike(rho; kwargs...)\n",
    "    #rho = [period, ecc, tp]\n",
    "    \n",
    "    # hard bounds\n",
    "    if rho[1] < 0.0\n",
    "        return Inf\n",
    "    end \n",
    "    if rho[2] >= 1.0\n",
    "        return Inf\n",
    "    end \n",
    "    if rho[2] < 0.0 \n",
    "        return Inf\n",
    "    end\n",
    "    \n",
    "    # call forward model \n",
    "    model, B = rv_forward(rho[1], rho[2], rho[3], time, rv, err);\n",
    "    \n",
    "    # Chi^2\n",
    "    return -loglike(rv, model, err);\n",
    "end "
