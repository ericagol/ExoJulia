    "function loglike(data, model, err)\n",
    "    # Chi^2\n",
    "    ll = 0.0;\n",
    "    for i=1:length(data)\n",
    "        ll -= 0.5 * (data[i] - model[i])^2 / (err[i])^2;\n",
    "    end\n",
    "    return ll\n",
    "end "
   ]
